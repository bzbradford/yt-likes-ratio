{
  "name": "YouTube Likes Ratio",
  "description": "Shows percent of viewers who liked the video. Useful proxy for the old like:dislike ratio.",
  "version": "1.0.0",
  "manifest_version": 3,
  "host_permissions": ["*://*.youtube.com/watch*"],
  "icons": {
    "16": "/icon-16.png",
    "32": "/icon-32.png",
    "48": "/icon-48.png",
    "128": "/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/watch*"],
      "js": ["yt_ratio.js"]
    }
  ]
}
